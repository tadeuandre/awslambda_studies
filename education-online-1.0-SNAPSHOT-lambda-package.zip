AWSTemplateFormatVersion: '2010-09-09'
Transform: AWS::Serverless-2016-10-31
Description: AWS Serverless Spring Boot 2 API - com.infnet::education-online
Globals:
  Api:
    EndpointConfiguration: REGIONAL
Resources:
  EducationOnlineFunction:
    Type: AWS::Serverless::Function
    Properties:
      Handler: com.infnet.StreamLambdaHandler::handleRequest
      Runtime: java8
      CodeUri: s3://education-online/b540c21257b896d88fb494db8d572916
      MemorySize: 512
      Policies: AWSLambdaBasicExecutionRole
      Timeout: 5000
      Events:
        GetResource:
          Type: Api
          Properties:
            Path: /{proxy+}
            Method: any
Outputs:
  EducationOnlineApi:
    Description: URL for application
    Value:
      Fn::Sub: https://${ServerlessRestApi}.execute-api.${AWS::Region}.amazonaws.com/Prod/ping
    Export:
      Name: EducationOnlineApi
